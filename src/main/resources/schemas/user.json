{
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "title": "User",
  "description": "A single user",
  "properties": {
    "id": {
      "type": "string",
      "title": "id"
    },
    "username": {
      "type": "string",
      "title": "username"
    },
    "password": {
      "type": [
        "null",
        "string"
      ],
      "title": "password"
    },
    "enabled": {
      "type": "boolean",
      "title": "enabled"
    },
    "lockedOut": {
      "type": "boolean",
      "title": "lockedOut"
    }
  },
  "required": [
    "id",
    "username"
  ]
}
