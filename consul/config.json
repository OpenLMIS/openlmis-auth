{
  "service": {
    "Name": "auth",
    "Id": "auth-service",
    "Port": 8080,
    "Tags": ["openlmis-service"],
    "check": {
      "interval": "10s",
      "http": "http://HOST:PORT/actuator/health",
      "timeout": "5s",
      "deregisterCriticalServiceAfter": "10m"
    }
  },
  "raml": "/consul/api-definition.yaml",
  "path": [
    "auth",
    "auth/docs",
    "auth/docs/<all>",
    "auth/webjars",
    "auth/webjars/<all>",
    "api/oauth",
    "api/oauth/<all>"
  ]
}
